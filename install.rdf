<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest"
			em:id="flashify@pseudodistant.dev" em:version="1.0" em:name="Flashify"
			em:description="Attempts to force websites to play Flash content in Adobe Flash Player instead of Ruffle."
			em:creator="PseudoDistant" 
			em:iconURL="chrome://flashify/skin/icon256.png"
			em:icon64URL="chrome://flashify/skin/icon64.png"
			em:homepageURL="https://pseudodistant.dev/flashify.html"
			em:unpack="false" em:type="2">
		<em:optionsURL>chrome://flashify/content/preferences.xul</em:optionsURL>
		<em:targetApplication name="Basilisk">
			<Description
				em:id="{ec8030f7-c20a-464f-9b0e-13a3a9e97384}" em:minVersion="52.0"
				em:maxVersion="52.*" />
		</em:targetApplication>
		<em:targetApplication name="Pale Moon">
			<Description
				em:id="{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}" em:minVersion="28.0.0a1" 
				em:maxVersion="33.*" />
		</em:targetApplication>
		<!-- SeaMonkey doesn't support Flash anymore.
		<em:targetApplication name="SeaMonkey">
			<Description
				em:id="92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a" em:minVersion="2.53.0"
				em:maxVersion="2.53.*" />
		</em:targetApplication>
		-->
	</Description>
</RDF>
